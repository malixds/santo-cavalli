# SMS.ru API Configuration
SMSRU_API_KEY=your_smsru_api_key_here
SMSRU_BASE_URL=https://sms.ru
SMSRU_CACHE_TTL=300

# ClickHouse Configuration (Native Client)
CLICKHOUSE_HOST=clickhouse
CLICKHOUSE_PORT=9000
CLICKHOUSE_USERNAME=default
CLICKHOUSE_PASSWORD=clickhouse
CLICKHOUSE_DATABASE=logs

# Kafka Configuration
KAFKA_BROKERS=kafka:9092
KAFKA_CONSUMER_GROUP_ID=main-service
KAFKA_LOGGING_ENABLED=true
KAFKA_LOGGING_LEVEL=info
